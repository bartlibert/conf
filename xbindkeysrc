"urxvtc"
  Mod4 + o

"firefox"
  Mod4 + b

"dmenu -fn '7x14' -p 'pass search' < /dev/null | xargs -I{} pass search -c {}"
  Mod4 + i

"dmenu -fn '7x14' -p 'pass search -u' < /dev/null | xargs -I{} pass search -u -c {}"
  Shift+Mod4 + i

"dmenu_run -fn '7x14'"
  Mod4 + r

"screenshot"
  Mod4 + s

"screenshot --full"
  Shift+Mod4 + s

"toggle_numkeys"
  Shift + Shift_R

"toggle_numkeys"
  Shift + Shift_L

"amixer set Master 1+"
  XF86AudioRaiseVolume

"amixer set Master 1-"
  XF86AudioLowerVolume

"amixer set Capture toggle"
  XF86AudioMicMute

"xrandr --auto"
  XF86Display

"xset dpms force off"
  XF86Launch3
